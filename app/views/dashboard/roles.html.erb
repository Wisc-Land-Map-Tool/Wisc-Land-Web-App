<div id="page-content-wrapper">
        <!-- Keep all page content within the page-content inset div! -->
        <div class="page-content inset">
        	<h1>All Users</h1>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= user.email %>
      <%= link_to "delete", user_path(id:user.id), method: :delete,
                                  data: { confirm: "You sure?" } %>

    </li>
  <% end %>
</ul>

        </div>
</div>